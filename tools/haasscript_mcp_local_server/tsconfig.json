{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2022", "dom"], // Changed to es2022
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "paths": {
      // Removed "@modelcontextprotocol/sdk/*": ["node_modules/@modelcontextprotocol/sdk/*"],
      // Removed "agents/*": ["node_modules/agents/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}