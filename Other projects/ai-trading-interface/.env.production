# Production Environment Configuration
NODE_ENV=production
VITE_BUILD_TARGET=desktop
VITE_APP_VERSION=1.0.0
VITE_APP_NAME="AI Trading Interface"

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_BASE_URL=ws://localhost:8080
VITE_RAG_API_URL=http://localhost:5000

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_CHAIN_OF_THOUGHT=true
VITE_ENABLE_GENERATIVE_UI=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Build Optimization
VITE_ENABLE_BUNDLE_ANALYZER=false
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_MINIFICATION=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS=false

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_ERROR_REPORTING=true