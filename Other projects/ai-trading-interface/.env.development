# Development Environment Configuration
NODE_ENV=development
VITE_BUILD_TARGET=desktop
VITE_APP_VERSION=1.0.0-dev
VITE_APP_NAME="AI Trading Interface (Dev)"

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_BASE_URL=ws://localhost:8080
VITE_RAG_API_URL=http://localhost:5000

# Feature Flags
VITE_ENABLE_VOICE_COMMANDS=true
VITE_ENABLE_CHAIN_OF_THOUGHT=true
VITE_ENABLE_GENERATIVE_UI=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Build Optimization
VITE_ENABLE_BUNDLE_ANALYZER=true
VITE_ENABLE_SOURCE_MAPS=true
VITE_ENABLE_MINIFICATION=false

# Security
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS=false

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_ERROR_REPORTING=false

# Development Tools
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEV_TOOLS=true